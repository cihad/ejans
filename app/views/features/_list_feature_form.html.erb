<%= feature.form_element.fields_for :list_feature do |f| %>
  <%= form_item_layout do %>
    <%= form_label_layout do %>
      <%= f.label feature.value, feature.label %>
    <% end %>
    <%= form_value_layout do %>
      <%= f.collection_select :list_item_ids, feature.child.list_items, :id, :name, {}, multiple: true %>  
      <%= feature.form_element.hidden_field :feature_configuration_id %>
    <% end %>
  <% end %>
<% end %>