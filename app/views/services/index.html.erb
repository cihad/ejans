<%= title "Listing Services" %>
<%= content_for :breadcrumb do %>
  &raquo; <span>Services</span>
<% end %>

<h2>Listing Services</h2>

<table>
  <tr>
    <th>Owner</th>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @services.each do |service| %>
  <tr>
    <td><%= service.owner_id %></td>
    <td><%= service.title %></td>
    <td><%= service.description %></td>
    <td><%= link_to 'Show', service %></td>
    <td><%= link_to 'Edit', edit_service_path(service) %></td>
    <td><%= link_to 'Destroy', service, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Service', new_service_path %>
