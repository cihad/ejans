<%= select_tag field.machine_name,
      options_for_select(
        field.list_items.inject({}) do |hash, item|
          hash[item.name] = item.id
          hash
        end,
        params["#{field.machine_name}"]),
      multiple: true,
      class: "input-mini #{field.machine_name}" %>

<%= multiselect '.' + field.machine_name %>