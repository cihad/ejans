<!DOCTYPE html>
<html>

<head>
  <title><%= yield(:title) || "The Place to Buy Stuff" %> - Ejans.com</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application", "jquery.tablesorter", "jquery.tablesorter.pager" %>
  <%= csrf_meta_tags %>
</head>
<!-- /HEAD -->

<body class="admin-page">
  <style>
    .container {
      width: 1060px;
    }
  </style>
  <!-- Container -->
  <div class="container">

    <%= render 'layouts/header' %>

    <div id="page">
      <div data-pjax-container>
        
        <div class="row">
          <% if breadcrumbs.items.size != 1 %>
          <div id="breadcrumb">
            <%= breadcrumbs.render %>
          </div>
          <!-- /Breadcrumb -->
          <% end %>
          


          <% if content_for?(:left) %>
          <div class="span4">
            <aside id="left">
              <%= yield :left %>
            </aside>
          </div>
          <!-- /aside left -->
          <% end %>

          <div class="span14">
            <section id="content">
              <%= render 'layouts/messages' %>
                <%= yield %>
            </section>
          </div>
          <!-- /Content -->



      </div>
    </div>
    <!-- /Page -->

    <%= render 'layouts/footer' %>

  </div>
  <!-- /Container -->

  <%= render 'layouts/signin' %>
  <%= yield :bottom %>

  <%#= debug(request) if Rails.env.development? %>
  <%#= debug(session) if Rails.env.development? %>
  <%#= debug(params) if Rails.env.development? %>

</body>
</html>
