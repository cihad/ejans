<%= select_tag  field.machine_name,
                options_for_select(
                  field.
                    parent_node_node_type.
                    nodes.
                    published.
                    inject({}) do |hash, node|
                      hash[node.title] = node.id; hash
                    end,
                  params[field.machine_name]
                ),
                multiple: true,
                class: field.machine_name %>

<%= multiselect ".#{field.machine_name}" %>