<%= remotipart_response do %>
  $('.images .not-uploaded').remove();
  
  <% @images.each do |image| %>
    $('.images').append( '<%= escape_javascript( render "images/image", mustache: image_for_mustache(image, @node) ) %>' );
  <% end %>

  $('#new_features_image').remove();
  $('#hidden-form').append( '<%= escape_javascript(render "images/form") %>' );
  console.log('yes');
<% end %>