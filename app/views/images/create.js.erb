<%= remotipart_response do %>
  $('.images .not-uploaded').remove();
  
  <% @images.each do |image| %>
    $('.images').append('<%= escape_javascript(render "image",
      mustache: image_for_mustache(image: image, node: @node, keyname: @keyname) ) %>');
  <% end %>

  $('#new_fields_image').remove();
  $('#hidden-form').append( '<%= escape_javascript(render "form", keyname: @keyname) %>' );
<% end %>