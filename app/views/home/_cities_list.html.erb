<div class="row">
  <% @cities.in_groups_of(@groups_of, false) do |group| %>
    <div class="span2">
      <ul>
      <% group.each do |city| %>
        <li <%= 'class=active' if city == current_place %>><%= link_to city.name, home_path(city) %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
</div>